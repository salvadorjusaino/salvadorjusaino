			"type": "text-and-comment",
			"foreground": "#000000",
			"background": "#ffc107",
			"iconColour": "#000000"
		},
		"NOTE": {
			//"type": "text-and-comment",
			"icon": "note",
			"foreground": "#ffffff",
			"background": "#808080",
			"iconColour": "#808080"
		}
	},
	"yaml.schemas": {
		"file:///Users/salvador.jusaino/.vscode/extensions/atlassian.atlascode-3.0.9/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
	},
	"editor.linkedEditing": true,
	"editor.matchBrackets": "never",
	"editor.bracketPairColorization.independentColorPoolPerBracketType": false,
	"editor.bracketPairColorization.enabled": true,
	"editor.guides.bracketPairs": true,
	"indentRainbow.indicatorStyle": "light",
	"indentRainbow.lightIndicatorStyleLineWidth": 1,
	"indentRainbow.colorOnWhiteSpaceOnly": true,
	"editor.guides.indentation": false,
	"indenticator.width": 1,
	"indenticator.color.dark": "rgba(255,255,255,0)",
	"indenticator.inner.color.light": "#999",
	"breadcrumbs.enabled": false
